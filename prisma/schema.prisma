generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id           String   @id @default(uuid())
  username     String   @unique
  password     String
  passwordSalt String
  userLevel    String?
  fullname     String?
  email        String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @default(now())

}

model listMenu {
  id       String @unique @default(uuid())
  menuName String
}

enum userLevel {
  ADMIN
  STUDENT
  TEACHER
}
