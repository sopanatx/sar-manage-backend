    "crypto-js": "^4.0.0",
    "csurf": "^1.11.0",
    "fs-capacitor": "^6.2.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "graphql-upload": "^12.0.0",
    "helmet": "^4.4.1",
    "minio": "^7.0.18",
    "nestjs-minio": "^2.1.7",
    "nestjs-throttler": "^0.3.0",
    "node-pre-gyp": "^0.17.0",
